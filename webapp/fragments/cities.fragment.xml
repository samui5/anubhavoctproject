<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
	<Select items="{/cities}">
		<items>
			<core:Item key="{name}" text="{name}"></core:Item>
		</items>
	</Select>
	<ComboBox items="{/cities}">
		<items>
			<core:Item key="{name}" text="{name}"></core:Item>
		</items>
	</ComboBox>
	<MultiComboBox items="{/cities}">
		<items>
			<core:Item key="{name}" text="{name}"></core:Item>
		</items>
	</MultiComboBox>
</core:FragmentDefinition>